version: 2.0
sandboxed: true
start: auto

executables:
{
	infraredTemperature = ( infraredTemperatureComponent )
}

processes:
{
	envVars:
	{
		LE_LOG_LEVEL = DEBUG
	}

	run:
	{
		( infraredTemperature )
	}
	faultAction: stopApp
}

requires:
{
	device:
	{
		[rw]	/dev/i2c-1	/dev/
	}
}

extern:
{
	infraredTemperature.infraredTemperatureComponent.ma_infraredTemperature
}